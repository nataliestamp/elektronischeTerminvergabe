        Patientennnummer = 100;
        Untersuchung = "MRT";
        Person1 = 8;
        Kalender1 = 5;
        
    }

   public Verwaltung(int Patientennummer,String Name,String Adresse,String Geburtsdatum,String E-Mail, 
